{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome against localhost",
        "url": "http://localhost:5173",
        "webRoot": "${workspaceFolder}/src"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Vite Debug",
        "program": "${workspaceFolder}/node_modules/vite/bin/vite.js",
        "cwd": "${workspaceFolder}",
        "console": "integratedTerminal"
      }
    ]
  }
  